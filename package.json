{
  "name": "baratona",
  "version": "1.0.4",
  "description": "Baratona electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "publish": "build -p always"
  },
  "repository": "https://github.com/rt3norio/baratona-electron",
  "keywords": [
    "Electron",
    "baratona",
    "funcional"
  ],
  "author": "Funcional <mnascimento@funcionalcorp.com.br>",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^5.0.2",
    "electron-builder": "^20.41.0"
  },
  "dependencies": {
    "chokidar": "^3.0.0",
    "electron-log": "^3.0.6",
    "electron-updater": "^4.0.6",
    "jQuery": "^1.7.4"
  },
  "build": {
    "appId": "baratona",
    "linux": {
      "category": "baratona",
      "target": "AppImage"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "rt3norio",
        "repo": "baratona-electron"
      }
    ]
  },
  "postinstall": "electron-builder install-app-deps"
}
